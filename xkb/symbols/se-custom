default partial alphanumeric_keys
xkb_symbols "se-custom" {
	include "se(nodeadkeys)"

	name[Group1]="Swedish (Programming)";

	// Swap $ Dollar sign and Â¤ Currency sign
	key <AE04> { [any, dollar, currency] };

	// Symbols in place of Swedish letters
	key <AD11> { [bracketleft, braceleft, aring, Aring] };
	key <AD12> { [bracketright, braceright, odiaeresis, Odiaeresis] };
	key <AC10> { [backslash, asciicircum, asciitilde] };
};
